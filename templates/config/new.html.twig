{% extends 'base.html.twig' %}

{% block title %} - {{ 'create'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'create'|trans }}</h1>

    {{ form_start(form, {
        action: path('config_edit', { id: 1 }),
        method: 'POST'
    }) }}
        {{ form_widget(form) }}
        {{ include('common/_form_actions.html.twig', {
            submit_label: 'create'|trans,
            back_href: path('settings_index'),
            back_label: 'back.tolist'|trans
        }) }}
    {{ form_end(form) }}
{% endblock %}
